{
    "name": "adwiv/laravel-crud-daisyui",
    "description": "Generate CRUD Model, Controller, Resource, Request and Views for Laravel with DaisyUI",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/framework": ">=11.0"
    },
    "autoload": {
        "psr-4": {
            "Adwiv\\Laravel\\CrudGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adwiv\\Laravel\\CrudGenerator\\CrudGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
